<p class="text-center text-muted">TinyApp</p>

    <% if(!user){ %>
        <p><a href = "/login">Login</a> <a href = "/register">Register</a></p>

    <% } else{ %>
        <form method="POST" action="/logout">
        <p>Hello, <%= user['email'] %>!</p><input type="submit" value="Logout"></form>
    <% } %>




